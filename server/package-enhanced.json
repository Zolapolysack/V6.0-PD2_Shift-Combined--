{
  "name": "pd2-sheets-proxy-enhanced",
  "version": "2.0.0",
  "description": "Enhanced Google Sheets Proxy API - Supports 20-40 concurrent users",
  "type": "module",
  "main": "index-enhanced.js",
  "scripts": {
    "start": "node index-enhanced.js",
    "start:cluster": "ENABLE_CLUSTER=true node index-enhanced.js",
    "start:single": "ENABLE_CLUSTER=false node index-enhanced.js",
    "dev": "NODE_ENV=development nodemon index-enhanced.js",
    "test": "node --test",
    "validate": "node ../scripts/validate-google-creds.js"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "googleapis": "^140.0.0",
    "dotenv": "^16.4.5",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2"
  },
  "devDependencies": {
    "nodemon": "^3.1.0"
  }
}
